gobject Animal [abstract] {
	
}
	
gobject Dog {

	inherits from Animal;
	
	public methods {
	
		Dog
			<- name: string [const];
		
	}
	
}
	
gobject Human [final] {
	
	inherits from Animal;
		
	properties {
		
		full-name {
			description = "full name",
			type = string,
			access = read-only
		}
			 
	}
		
	public methods {
		
		Human 
			<- first_name: string [const]
			<- last_name: string [const];
				
	}
		
}
